{
  "@schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "addtionalProperties": false,
  "properties": {
    "src": {
      "type": "string",
      "default": ""
    },
    "state": {
      "type": "string",
      "enum": ["running", "stopped"],
      "default": "stopped"
    }
  },
  "required": [ "src", "state" ]
}
